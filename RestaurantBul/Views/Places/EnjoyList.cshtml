
@model List<RestaurantBul.Models.Place>

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="Surjith S M">
    <meta name="description" content="#">
    <meta name="keywords" content="#">

    <script src="../../cdn-cgi/apps/head/-mEFVS8y7qx5pVzWHQTCQu5gnVM.js"></script>
    <link rel="shortcut icon" href="#">

    <title>Listed</title>

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/bootstrap.min.css">

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/themify-icons.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/simple-line-icons.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/font-awesome.min.css">

    <link href="~/Content/tempp/Sablon/listed/css/set1.css" rel="stylesheet">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/style.css">
</head>

<body>



    <section class="featured-wrap">
        <div class="container-fluid container-subpage">
            <div class="row">
                <div class="col-md-12 responsive-wrap">
                    <div class="row detail-filter-wrap">
                        <div class="row">

                            @foreach (var item in Model)
                            {

                                <div class="col-md-4 card-2">

                                    <div class="card">

                                        <a href="@Url.Action("PlaceDetails","Places", new { id = item.PlaceId})"><img class="card-img-top" src="@item.MenuPic" alt="Card image cap"></a>
                                        <div class="card-body">
                                            <h5 class="card-title">@item.PlaceName</h5>
                                            <ul class="card-rating">
                                                <li>5.0</li>
                                                <li><a href="@Url.Action("Index","Comments")" style=" color:tomato">Yorumlar</a></li>
                                                <li><i class="fa fa-circle" aria-hidden="true"></i></li>
                                                <li>@item.AvgPrice</li>
                                                <li><i class="fa fa-circle" aria-hidden="true"></i></li>
                                                <li>
                                                    <svg xmlns="http://www.w3.org/2000/svg" class="card-svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" viewBox="0 0 470 470" enable-background="new 0 0 470 470" width="512px" height="512px">
                                                        <g>
                                                            <g>
